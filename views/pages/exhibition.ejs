                <a-asset-item id="mplant-m" src="/static/assets/3d/money_plant/eb_house_plant_01.mtl"></a-assets-item>
              </a-assets>


              <!-- sky -->
              <a-sky src="#sky_texture"></a-sky>
              <!-- plane -->
              <a-plane static-body rotation="-90 0 0" material="src:#grass_texture;repeat:500 500" scale="1000 1000 0" position="0 0 0"></a-plane>

              <!-- walls -->
              <a-box static-body color="gray" depth="8" height="4" width="6" position="0 2 -5"></a-box>
              <a-box static-body color="gray" depth="0.5" height="3.5" width="20" position="7 1.75 -3" rotation="0 90 0"></a-box>
              <a-box static-body color="gray" depth="0.5" height="3.5" width="20" position="-7 1.75 -3" rotation="0 90 0"></a-box>
              <a-box static-body color="gray" depth="0.5" height="4" width="14.5" position="0 2 -9.5"></a-box>
              <a-box static-body color="gray" depth="0.5" height="4" width="14.5" position="0 2 7"></a-box>

              <!-- roof -->
              <a-box src="#concrete_texture" depth="20" height="0.5" width="14" position="0 4.3 0"></a-box>
              
              <a-plane src="#white_marbel" height="20" width="14" position="0 0.2 0" rotation="-90 0 0"></a-plane>

              <!-- interiors -->
              <a-image width="2" height="1.5" position="0 3 -0.8" src="#logo"></a-image>
              <a-text value="
              MUKTOKISHOR PHOTO EXHIBITION
              " color="darkblue" position="0 1.4 -0.8" align="center"></a-text>

              <a-image width="1.5" height="2" position="6.4 1.2 2" rotation="-10 -90 0" src="#abd7"></a-image>
              <a-image width="1.5" height="2" position="6.4 1.2 4" rotation="-10 -90 0" src="#abd10"></a-image>
              <a-image width="1.5" height="2" position="-6.5 1.2 4" rotation="-10 90 0" src="#abd9"></a-image>
              <a-image width="1.5" height="2" position="-6.5 1.2 2" rotation="-10 90 0" src="#abd8"></a-image>

              <a-image width="1" height="0.75" position="0 3 6.5" src="#around_bd" rotation="0 180 0"></a-image>

              <a-image width="1" height="0.75" position="3 2 6.5" src="#abd1" rotation="0 180 0"></a-image>
              <a-image width="1" height="0.75" position="1 2 6.5" src="#abd2" rotation="0 180 0"></a-image>
              <a-image width="1" height="0.75" position="-1 2 6.5" src="#abd3" rotation="0 180 0"></a-image>
              <a-image width="1" height="0.75" position="-3 2 6.5" src="#abd4" rotation="0 180 0"></a-image>

              <a-entity obj-model="obj: #plant-o; mtl: #plant-m" position="-5 0.25 -8.7" scale="0.25 0.25 0.25"></a-entity>
              <a-entity obj-model="obj: #plant-o; mtl: #plant-m" position="4.7 0.25 -8.7" scale="0.25 0.25 0.25"></a-entity>



              <!-- photos -->
              <a-image width="1" height="0.75" position="-3.02 2 -7" src="#tasnim_toa" rotation="0 90 0"></a-image>
              <a-text value="
              Tasnim Toa
              " color="white" align="center" position="-3.02 1.5 -7" rotation="0 270 0"></a-text>
              
              <a-image width="1" height="0.75" position="-3.02 2 -5" src="#sinthiya_tasnim" rotation="0 90 0"></a-image>
